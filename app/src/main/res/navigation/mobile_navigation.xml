<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.techton.travel.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_navi"
        android:name="com.techton.travel.ui.navi.NaviFragment"
        android:label="Navi"
        tools:layout="@layout/fragment_navi" />

    <fragment
        android:id="@+id/navigation_calendar"
        android:name="com.techton.travel.ui.calendar.CalendarFragment"
        android:label="Calendar"
        tools:layout="@layout/fragment_calendar" />

    <fragment
        android:id="@+id/navigation_me"
        android:name="com.techton.travel.ui.me.MeFragment"
        android:label="Me"
        tools:layout="@layout/fragment_me" />
</navigation>